@Master['master.sshtml']

@Section['Content']
	<div ng-app ng-controller="AllPicsCtrl">
	<p>all images</p>
	<h1>{{images.count}}</h1>
	<input ng-model="imageFilter"/>
	<ol>
		<li ng-repeat="image in images.items | filter : imageFilter">
			<div>
				<a href="{{image.url}}">
					<h3>{{image.name}}</h3>
					<h5>{{image.date}}</h5>
				</a>
			</div>
		</li>
	<ol>
	</div>
@EndSection
